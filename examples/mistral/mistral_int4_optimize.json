{
    "input_model": { "type": "HfModel", "config": { "model_path": "mistralai/Mistral-7B-v0.1" } },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": { "accelerators": [ { "device": "cpu", "execution_providers": [ "CPUExecutionProvider" ] } ] }
        }
    },
    "data_configs": [
        { "name": "transformer_token_dummy_data", "type": "TransformersTokenDummyDataContainer" },
        {
            "name": "inc_static_quant_data_config",
            "type": "HuggingfaceContainer",
            "user_script": "user_script.py",
            "load_dataset_config": { "type": "simple_dataset" },
            "pre_process_data_config": { "type": "skip_pre_process" },
            "post_process_data_config": { "type": "skip_post_process" },
            "dataloader_config": { "type": "mistralai_calib_dataloader", "params": {"batch_size": 1} }
        }
    ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [ { "name": "avg", "priority": 1 } ],
                    "data_config": "transformer_token_dummy_data",
                    "user_config": {
                        "inference_settings": { "onnx": { "session_options": { "enable_profiling": false } } }
                    }
                }
            ]
        }
    },
    "passes": {
        "convert": {
            "type": "OptimumConversion",
            "config": { "target_opset": 14, "extra_args": { "legacy": false, "no_post_process": false } }
        },
        "optimize": {
            "type": "OrtTransformersOptimization",
            "config": {
                "model_type": "gpt2",
                "use_gpu": false,
                "keep_io_types": true,
                "optimization_options": { "use_multi_head_attention": false },
                "save_as_external_data": true,
                "all_tensors_to_one_file": true
            }
        },
        "quantization": {
            "type": "IncStaticQuantization",
            "config": {
                "approach": "weight_only",
                "weight_only_config": { "bits": 4, "algorithm": "GPTQ" },
                "recipes": { "gptq_args": { "accuracy_level": 0 } },
                "data_config": "inc_static_quant_data_config",
                "calibration_sampling_size": [ 8 ],
                "save_as_external_data": true,
                "all_tensors_to_one_file": true,
                "diagnosis": false
            }
        }
    },
    "pass_flows": [ [ "convert", "optimize", "quantization" ] ],
    "engine": {
        "evaluate_input_model": false,
        "evaluator": "common_evaluator",
        "host": "local_system",
        "target": "local_system",
        "cache_dir": "cache",
        "output_dir": "models",
        "output_name": "mistral_int4"
    }
}
